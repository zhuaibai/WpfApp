<UserControl
    x:Class="WpfApp1.UserControls.PTF_Monitor"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:WpfApp1.UserControls"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="800"
    d:DesignWidth="1200"
    Background="White"
    mc:Ignorable="d">
    <UserControl.Resources>
        <Style TargetType="GroupBox">
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="GroupBox">
                        <Grid Width="{TemplateBinding Width}" Height="{TemplateBinding Height}">
                            <StackPanel>
                                <Border Height="22" Background="#FFD9E8FF">
                                    <TextBlock
                                        HorizontalAlignment="Center"
                                        VerticalAlignment="Center"
                                        FontSize="16"
                                        FontWeight="Bold"
                                        Text="{TemplateBinding Header}" />
                                </Border>

                                <ContentPresenter />


                            </StackPanel>

                        </Grid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
        <Style TargetType="RadioButton">
            <Setter Property="Width" Value="90" />
            <Setter Property="Height" Value="30" />
            <Setter Property="Background" Value="#ddd" />
            <Setter Property="Foreground" Value="#999" />
            <Setter Property="BorderBrush" Value="#33ffffff" />
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="RadioButton">
                        <Border
                            x:Name="bor"
                            Background="{TemplateBinding Background}"
                            BorderBrush="#ddd"
                            BorderThickness="0,0,0,3">
                            <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center" />
                        </Border>
                        <ControlTemplate.Triggers>
                            <Trigger Property="IsChecked" Value="True">
                                <Setter TargetName="bor" Property="BorderThickness" Value="0,0,0,3" />
                                <Setter TargetName="bor" Property="BorderBrush" Value="#FF1677FF" />
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
            <Style.Triggers>
                <Trigger Property="IsChecked" Value="True">
                    <Setter Property="Foreground" Value="#FF1677FF" />
                </Trigger>
            </Style.Triggers>
        </Style>
    </UserControl.Resources>
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="4*" />
            <ColumnDefinition Width="5*" />
            <ColumnDefinition Width="4*" />
            <ColumnDefinition Width="auto" />

        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="30" />
            <RowDefinition />
            <RowDefinition Height="40" />
        </Grid.RowDefinitions>
        <!--  第一行  -->
        <StackPanel Orientation="Horizontal">
            <RadioButton
                Click="ElementView_Click"
                Content="{DynamicResource 概览图}"
                IsChecked="True" />
            <RadioButton Click="ElementSetting_Click" Content="{DynamicResource 参数设置}" />
            <RadioButton Content="{DynamicResource 软件升级}" />
        </StackPanel>
        <!--  第二行 第一列  -->
        <Grid Grid.Row="1" Margin="10,10">
            <Grid.RowDefinitions>
                <RowDefinition Height="auto" />
                <RowDefinition Height="auto" />
                <RowDefinition />
            </Grid.RowDefinitions>
            <!--  并网功能配置  -->
            <GroupBox Header="并网功能配置">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition />
                        <RowDefinition />
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="2*" />
                        <ColumnDefinition Width="1*" />
                        <ColumnDefinition Width="2*" />
                        <ColumnDefinition Width="1*" />
                    </Grid.ColumnDefinitions>
                    <!--  第一行  -->
                    <TextBlock Style="{StaticResource ContentText}" Text="{DynamicResource 并网功能}" />
                    <TextBlock
                        Grid.Column="1"
                        Style="{StaticResource ContentText}"
                        Text="{Binding HEEP1_PDF.GridConnectedFunction}" />
                    <TextBlock
                        Grid.Column="2"
                        Style="{StaticResource ContentText}"
                        Text="{DynamicResource CT功能开关}" />
                    <TextBlock
                        Grid.Column="3"
                        Style="{StaticResource ContentText}"
                        Text="{Binding HEEP1_PDF.CT_Enable}" />
                    <!--  第二行  -->
                    <TextBlock
                        Grid.Row="1"
                        Style="{StaticResource ContentText}"
                        Text="{DynamicResource 并网协议}" />
                    <TextBlock
                        Grid.Row="1"
                        Grid.Column="1"
                        Style="{StaticResource ContentText}"
                        Text="{Binding HEEP1_PDF.PV_GridConnectionProtocol}" />
                    
                </Grid>
            </GroupBox>
            <!--  市电  -->
            <ScrollViewer Grid.Row="1" VerticalScrollBarVisibility="Auto">
                <GroupBox Grid.Row="2" Header="市电">
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="2*" />
                            <ColumnDefinition Width="2*" />
                            <ColumnDefinition Width="1*" />
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                        </Grid.RowDefinitions>
                        <!--  标题  -->
                        <TextBlock
                            Grid.Row="0"
                            Style="{StaticResource HeaderText}"
                            Text="属性" />
                        <TextBlock
                            Grid.Row="0"
                            Grid.Column="1"
                            Style="{StaticResource HeaderText}"
                            Text="当前值" />
                        <TextBlock
                            Grid.Row="0"
                            Grid.Column="2"
                            Style="{StaticResource HeaderText}"
                            Text="单位" />
                        <!--  第一行  -->
                        <TextBlock
                            Grid.Row="1"
                            Style="{StaticResource ContentText}"
                            Text="{DynamicResource 市电电压}" />
                        <TextBlock
                            Grid.Row="1"
                            Grid.Column="1"
                            Style="{StaticResource ContentText}"
                            Text="{Binding HGRID_PDF.MainsVoltage}" />
                        <TextBlock
                            Grid.Row="2"
                            Style="{StaticResource ContentText}"
                            Text="{DynamicResource 市电功率}" />
                        <TextBlock
                            Grid.Row="2"
                            Grid.Column="1"
                            Style="{StaticResource ContentText}"
                            Text="{Binding HGRID_PDF.ACPower}" />
                        <!--  市电频率  -->
                        <TextBlock
                            Grid.Row="3"
                            Style="{StaticResource ContentText}"
                            Text="{DynamicResource 市电频率}" />
                        <TextBlock
                            Grid.Row="3"
                            Grid.Column="1"
                            Style="{StaticResource ContentText}"
                            Text="{Binding HGRID_PDF.MainsFrequency}" />
                        
                        <!--  当前允许最大逆变功率  -->
                        <TextBlock
                            Grid.Row="5"
                            Style="{StaticResource ContentText}"
                            Text="{DynamicResource 当前允许最大逆变功率}" />
                        <TextBlock
                            Grid.Row="5"
                            Grid.Column="1"
                            Style="{StaticResource ContentText}"
                            Text="{Binding HGRID_PDF.MaxInvPower}" />
                        <!--  并网电流  -->
                        <TextBlock
                            Grid.Row="6"
                            Style="{StaticResource ContentText}"
                            Text="{DynamicResource 并网电流}" />
                        <TextBlock
                            Grid.Row="6"
                            Grid.Column="1"
                            Style="{StaticResource ContentText}"
                            Text="{Binding HEEP1_PDF.GridCurrent}" />
                        <!--  调零功率  -->
                        <TextBlock
                            Grid.Row="7"
                            Style="{StaticResource ContentText}"
                            Text="{DynamicResource 调零功率}" />
                        <TextBlock
                            Grid.Row="7"
                            Grid.Column="1"
                            Style="{StaticResource ContentText}"
                            Text="{Binding HEEP3_PDF.ZeroAdjPwr}" />
                    </Grid>
                </GroupBox>
            </ScrollViewer>

            <!--  系统信息与状态  -->
            <ScrollViewer
                Grid.Row="2"
                VerticalAlignment="Bottom"
                VerticalScrollBarVisibility="Auto">
                <GroupBox FontSize="16" Header="系统信息与状态">
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition />
                            <ColumnDefinition />
                            <ColumnDefinition />
                            <ColumnDefinition />
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                        </Grid.RowDefinitions>
                        <!--  第一行  -->
                        <TextBlock Style="{StaticResource ContentText}" Text="{DynamicResource 系统时间}" />
                        <TextBlock
                            Grid.Column="1"
                            Style="{StaticResource ContentText}"
                            Text="{Binding HGEN.SystemTime}" />
                        <TextBlock
                            Grid.Column="2"
                            Style="{StaticResource ContentText}"
                            Text="{DynamicResource 版本号}" />
                        <TextBlock
                            Grid.Column="3"
                            Style="{StaticResource ContentText}"
                            Text="{Binding HIMSG1.Version}" />
                        <!--  第二行  -->
                        <TextBlock
                            Grid.Row="1"
                            Style="{StaticResource ContentText}"
                            Text="{DynamicResource 发布日期}" />
                        <TextBlock
                            Grid.Row="1"
                            Grid.Column="1"
                            Style="{StaticResource ContentText}"
                            Text="{Binding HIMSG1.ReleaseDate}" />
                       
                    </Grid>
                </GroupBox>
            </ScrollViewer>

        </Grid>
        <!--  第二行 第二列  -->
        <Grid Grid.Row="1" Grid.Column="1">
            <Grid.RowDefinitions>
                <RowDefinition Height="4*" MaxHeight="330" />
                <RowDefinition Height="auto" />
                <RowDefinition Height="3*" />
            </Grid.RowDefinitions>
            <!--  第一行 图标显示  -->
            <Grid >
                <Grid.RowDefinitions >
                    <RowDefinition  Height="1*"/>
                    <RowDefinition  Height="1*"/>
                    <RowDefinition  Height="1*"/>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition></ColumnDefinition>
                    <ColumnDefinition></ColumnDefinition>
                    <ColumnDefinition></ColumnDefinition>
                </Grid.ColumnDefinitions>
                <!--太阳能发电-->
                <local:RingUC icon="&#xe809;"  FontSize="12" PercentValue="{Binding pValue}"  Grid.Column="1"></local:RingUC>
                <!--左一-->
                <local:RingUC icon="&#xe61c;"  FontSize="12" PercentValue="50" Grid.Row="1"></local:RingUC>
                <!--中间-->
                <StackPanel Grid.Row="1" Grid.Column="1" VerticalAlignment="Center" HorizontalAlignment="Center">

                    <TextBlock HorizontalAlignment="Center" Foreground="#FF1BD0E4" Text="&#xe62f;" FontSize="15" FontFamily="../Resource/Fonts/#iconfont"></TextBlock>
                    <StackPanel Orientation="Horizontal" Margin="10" HorizontalAlignment="Center">
                        <TextBlock  Text="&#xec63;" FontSize="15" VerticalAlignment="Center" FontFamily="../Resource/Fonts/#iconfont" Foreground="#FF1BD0E4"></TextBlock>
                        <TextBlock  Text="&#xec63;" FontSize="15" VerticalAlignment="Center" FontFamily="../Resource/Fonts/#iconfont" Foreground="#FF1BD0E4"></TextBlock>
                        <TextBlock  Text="&#xec63;" FontSize="15" VerticalAlignment="Center" FontFamily="../Resource/Fonts/#iconfont" Foreground="#FF1BD0E4"></TextBlock>
                        <TextBlock  Text="&#xec63;" FontSize="15" VerticalAlignment="Center" FontFamily="../Resource/Fonts/#iconfont" Foreground="#FF1BD0E4"></TextBlock>
                        <TextBlock  Margin="10,0" Text="&#xe6af;" FontSize="30" FontFamily="../Resource/Fonts/#iconfont"></TextBlock>
                        <TextBlock  Text="&#xec62;" FontSize="15" VerticalAlignment="Center" FontFamily="../Resource/Fonts/#iconfont" Foreground="#FF1BD0E4"></TextBlock>
                        <TextBlock  Text="&#xec62;" FontSize="15" VerticalAlignment="Center" FontFamily="../Resource/Fonts/#iconfont" Foreground="#FF1BD0E4"></TextBlock>
                        <TextBlock  Text="&#xec62;" FontSize="15" VerticalAlignment="Center" FontFamily="../Resource/Fonts/#iconfont" Foreground="#FF1BD0E4"></TextBlock>
                        <TextBlock  Text="&#xec62;" FontSize="15" VerticalAlignment="Center" FontFamily="../Resource/Fonts/#iconfont" Foreground="#FF1BD0E4"></TextBlock>
                    </StackPanel>
                    <TextBlock HorizontalAlignment="Center" Foreground="#FF1BD0E4" Text="&#xe62f;" FontSize="15" FontFamily="../Resource/Fonts/#iconfont"></TextBlock>
                </StackPanel>
                <!--右一-->
                <local:RingUC icon="&#xe603;"  FontSize="12" PercentValue="78"  Grid.Column="2" Grid.Row="1"></local:RingUC>
                <!--中下-->
                <local:RingUC icon="&#xe973;"  FontSize="12" PercentValue="100" Grid.Column="1" Grid.Row="2"></local:RingUC>
                
            </Grid>
            <!--  第二行  -->
            <GroupBox Grid.Row="1" Header="PV">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="2*" />
                        <ColumnDefinition Width="2*" />
                        <ColumnDefinition Width="1*" />
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                    </Grid.RowDefinitions>
                    <!--  标题  -->
                    <TextBlock
                        Grid.Row="0"
                        Style="{StaticResource HeaderText}"
                        Text="属性" />
                    <TextBlock
                        Grid.Row="0"
                        Grid.Column="1"
                        Style="{StaticResource HeaderText}"
                        Text="当前值" />
                    <TextBlock
                        Grid.Row="0"
                        Grid.Column="2"
                        Style="{StaticResource HeaderText}"
                        Text="单位" />
                    <!--  PV电流  -->
                    <TextBlock
                        Grid.Row="1"
                        Style="{StaticResource ContentText}"
                        Text="{DynamicResource PV电流}" />
                    <TextBlock
                        Grid.Row="1"
                        Grid.Column="1"
                        Style="{StaticResource ContentText}"
                        Text="{Binding HPV_PDF.PVCurr}" />
                    <TextBlock
                        Grid.Row="1"
                        Grid.Column="2"
                        Style="{StaticResource ContentText}"
                        Text="A" />
                    <!--  PV电压  -->
                    <TextBlock
                        Grid.Row="2"
                        Style="{StaticResource ContentText}"
                        Text="{DynamicResource PV电压}" />
                    <TextBlock
                        Grid.Row="2"
                        Grid.Column="1"
                        Style="{StaticResource ContentText}"
                        Text="{Binding HPV_PDF.PVVolt}" />
                    <!--  PV功率  -->
                    <TextBlock
                        Grid.Row="3"
                        Style="{StaticResource ContentText}"
                        Text="{DynamicResource PV功率}" />
                    <TextBlock
                        Grid.Row="3"
                        Grid.Column="1"
                        Style="{StaticResource ContentText}"
                        Text="{Binding HPV_PDF.PVPwr}" />
                </Grid>
            </GroupBox>
            <!--  第三行  -->

            <GroupBox Grid.Row="2" Header="CT">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="2*" />
                        <ColumnDefinition Width="2*" />
                        <ColumnDefinition Width="*" />
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                    </Grid.RowDefinitions>
                    <!--  标题  -->
                    <TextBlock
                        Grid.Row="0"
                        Style="{StaticResource HeaderText}"
                        Text="属性" />
                    <TextBlock
                        Grid.Row="0"
                        Grid.Column="1"
                        Style="{StaticResource HeaderText}"
                        Text="当前值" />
                    <TextBlock
                        Grid.Row="0"
                        Grid.Column="2"
                        Style="{StaticResource HeaderText}"
                        Text="单位" />
                    <!--  CT电流  -->
                    <TextBlock
                        Grid.Row="1"
                        Style="{StaticResource ContentText}"
                        Text="{DynamicResource CT电流}" />
                    <TextBlock
                        Grid.Row="1"
                        Grid.Column="1"
                        Style="{StaticResource ContentText}"
                        Text="{Binding HCTMSG1_PDF.CTCurr}" />
                    <!--  CT功率  -->
                    <TextBlock
                        Grid.Row="2"
                        Style="{StaticResource ContentText}"
                        Text="{DynamicResource CT功率}" />
                    <TextBlock
                        Grid.Row="2"
                        Grid.Column="1 "
                        Style="{StaticResource ContentText}"
                        Text="{Binding HCTMSG1_PDF.CTPwr}" />
                </Grid>

            </GroupBox>
        </Grid>

        <!--  第二行 第三列  -->
        <Grid
            Grid.Row="1"
            Grid.Column="2"
            Margin="10">
            <Grid.RowDefinitions>
                <RowDefinition Height="auto" />
                <RowDefinition />
            </Grid.RowDefinitions>
            <GroupBox Header="输出">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="2*" />
                        <ColumnDefinition Width="2*" />
                        <ColumnDefinition Width="*" />
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                    </Grid.RowDefinitions>
                    <!--  标题  -->
                    <TextBlock
                        Grid.Row="0"
                        Style="{StaticResource HeaderText}"
                        Text="属性" />
                    <TextBlock
                        Grid.Row="0"
                        Grid.Column="1"
                        Style="{StaticResource HeaderText}"
                        Text="当前值" />
                    <TextBlock
                        Grid.Row="0"
                        Grid.Column="2"
                        Style="{StaticResource HeaderText}"
                        Text="单位" />
                    <!--  输出频率  -->
                    <TextBlock
                        Grid.Row="2"
                        Style="{StaticResource ContentText}"
                        Text="{DynamicResource 输出频率}" />
                    <TextBlock
                        Grid.Row="2"
                        Grid.Column="1"
                        Style="{StaticResource ContentText}"
                        Text="{Binding HOP_PDF.OutFreq}" />
                    <!--  视在功率  -->
                    <TextBlock
                        Grid.Row="3"
                        Style="{StaticResource ContentText}"
                        Text="{DynamicResource 视在功率}" />
                    <TextBlock
                        Grid.Row="3"
                        Grid.Column="1"
                        Style="{StaticResource ContentText}"
                        Text="{Binding HOP_PDF.ApparentPwr}" />
                    <!--  有功功率  -->
                    <TextBlock
                        Grid.Row="4"
                        Style="{StaticResource ContentText}"
                        Text="{DynamicResource 有功功率}" />
                    <TextBlock
                        Grid.Row="4"
                        Grid.Column="1"
                        Style="{StaticResource ContentText}"
                        Text="{Binding HOP_PDF.ActivePwr}" />

                    <!--  负载百分比  -->
                    <TextBlock
                        Grid.Row="5"
                        Style="{StaticResource ContentText}"
                        Text="{DynamicResource 负载百分比}" />
                    <TextBlock
                        Grid.Row="5"
                        Grid.Column="1"
                        Style="{StaticResource ContentText}"
                        Text="{Binding HOP_PDF.LoadPercent}" />
                    <!--  直流分量  -->
                    <TextBlock
                        Grid.Row="6"
                        Style="{StaticResource ContentText}"
                        Text="{DynamicResource 直流分量}" />
                    <TextBlock
                        Grid.Row="6"
                        Grid.Column="1"
                        Style="{StaticResource ContentText}"
                        Text="{Binding HOP_PDF.DCOffset}" />
                    <!--  电感电流  -->
                    <TextBlock
                        Grid.Row="7"
                        Style="{StaticResource ContentText}"
                        Text="{DynamicResource 电感电流}" />
                    <TextBlock
                        Grid.Row="7"
                        Grid.Column="1"
                        Style="{StaticResource ContentText}"
                        Text="{Binding HOP_PDF.InductorCurr}" />
                    <!--  电感功率  -->
                    <TextBlock
                        Grid.Row="8"
                        Style="{StaticResource ContentText}"
                        Text="{DynamicResource 电感功率}" />
                    <TextBlock
                        Grid.Row="8"
                        Grid.Column="1"
                        Style="{StaticResource ContentText}"
                        Text="{Binding HOP_PDF.InductorPwr}" />
                    <!--  输出电压  -->
                    <TextBlock
                        Grid.Row="1"
                        Style="{StaticResource ContentText}"
                        Text="{DynamicResource 输出电压}" />
                    <TextBlock
                        Grid.Row="1"
                        Grid.Column="1"
                        Style="{StaticResource ContentText}"
                        Text="{Binding HOP_PDF.OutVolt}" />
                    
                </Grid>
            </GroupBox>
            <ScrollViewer Grid.Row="1" VerticalScrollBarVisibility="Auto" VerticalAlignment="Bottom">
                <GroupBox FontSize="16" Header="机器状态">
                    <Grid Margin="0,10,0,0">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="2*" />
                            <ColumnDefinition Width="1*" />
                            <ColumnDefinition Width="2*" />
                            <ColumnDefinition Width="1*" />
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                        </Grid.RowDefinitions>

                        

                        <!--  母线电压  -->
                        <TextBlock
                            Grid.Row="1"
                            Style="{StaticResource ContentText}"
                            Text="{DynamicResource 母线电压}" />
                        <TextBlock
                            Grid.Row="1"
                            Grid.Column="1"
                            Style="{StaticResource ContentText}"
                            Text="{Binding HBAT_PDF.BusVolt}" />
                        <!--  PFC工作状态  -->
                        <TextBlock
                            Grid.Row="1"
                            Grid.Column="2"
                            Style="{StaticResource ContentText}"
                            Text="{DynamicResource PFC工作状态}" />
                        <TextBlock
                            Grid.Row="1"
                            Grid.Column="3"
                            Style="{StaticResource ContentText}"
                            Text="{Binding HBAT_PDF.PFCStatus}" />
                        <!--  逆变温度  -->
                        <TextBlock
                            Grid.Row="2"
                            Style="{StaticResource ContentText}"
                            Text="{DynamicResource 逆变温度}" />
                        <TextBlock
                            Grid.Row="2"
                            Grid.Column="1"
                            Style="{StaticResource ContentText}"
                            Text="{Binding HTEMP_PDF.InvTemp}" />
                        <!--  当前最高温度  -->
                        <TextBlock
                            Grid.Row="2"
                            Grid.Column="2"
                            Style="{StaticResource ContentText}"
                            Text="{DynamicResource 当前最高温度}" />
                        <TextBlock
                            Grid.Row="2"
                            Grid.Column="3"
                            Style="{StaticResource ContentText}"
                            Text="{Binding HTEMP_PDF.MaxTemp}" />
                        <!--  风扇转速  -->
                        <TextBlock
                            Grid.Row="3"
                            Style="{StaticResource ContentText}"
                            Text="{DynamicResource 风扇转速}" />
                        <TextBlock
                            Grid.Row="3"
                            Grid.Column="1"
                            Style="{StaticResource ContentText}"
                            Text="{Binding HTEMP_PDF.FanSpeed}" />
                        <!--  风扇使能  -->
                        <TextBlock
                            Grid.Row="3"
                            Grid.Column="2"
                            Style="{StaticResource ContentText}"
                            Text="{DynamicResource 风扇使能}" />

                        <TextBlock
                            Grid.Row="3"
                            Grid.Column="3"
                            Style="{StaticResource ContentText}"
                            Text="{Binding HTEMP_PDF.FanEnable}" />


                    </Grid>
                </GroupBox>
            </ScrollViewer>



        </Grid>

        <!--  第三行  -->
        <Grid Grid.Row="2" Grid.ColumnSpan="4">
            <Border BorderBrush="Gray" BorderThickness="1">
                <StackPanel Margin="5,0" Orientation="Horizontal">
                    <Button
                        x:Name="StopBtn"
                        Width="80"
                        Height="30"
                        Margin="0,0,5,0"
                        Background="Red"
                        Command="{Binding DataContext.StopCommand, RelativeSource={RelativeSource AncestorType=Window}}"
                        Content="{DynamicResource 停止}"
                        Style="{StaticResource BtnStyle}"
                        Tag="&#xe691;" />
                    <Button
                        x:Name="BeginBtn"
                        Width="80"
                        Height="30"
                        Background="#FF1AFA29"
                        Command="{Binding DataContext.StartCommand, RelativeSource={RelativeSource AncestorType=Window}}"
                        Content="{DynamicResource 开始}"
                        Style="{StaticResource BtnStyle}"
                        Tag="&#xe622;" />
                    <Border
                        Width="2"
                        Margin="5,0,0,0"
                        Background="Black" />
                    <CheckBox Margin="10,0,0,0" VerticalAlignment="Center">
                        <TextBlock
                            VerticalAlignment="Center"
                            FontSize="15"
                            Text="导出运行数据" />
                    </CheckBox>
                    <TextBlock Margin="5,0,0,0" VerticalAlignment="Center">
                        <Hyperlink>日志目录</Hyperlink>
                    </TextBlock>
                    <Border
                        Width="2"
                        Margin="5,0,0,0"
                        Background="Black" />
                </StackPanel>
            </Border>
        </Grid>



        <!--  参数设置 界面  -->
        <Grid
            x:Name="SettingView"
            Grid.Row="1"
            Grid.RowSpan="2"
            Grid.ColumnSpan="4"
            Background="White"
            Visibility="Collapsed">
            <ScrollViewer VerticalScrollBarVisibility="Hidden">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="3*" />
                        <ColumnDefinition Width="3*" />
                        <ColumnDefinition Width="2*" />
                    </Grid.ColumnDefinitions>
                    <!--  第一列  -->
                    <Border
                        Margin="3"
                        Padding="20"
                        Background="#FFF0F0F0"
                        CornerRadius="5">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
